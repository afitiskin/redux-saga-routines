    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-eslint": "^8.1.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "mocha": "^5.2.0",
    "redux-saga": ">=0.15.0",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.7"
  },
  "peerDependencies": {
    "@babel/runtime":  ">=7.0.0",
    "redux-saga": ">=0.15.0"
